#include<stdio.h>
#include <iostream>
using namespace std;
//空间大的最简单的排序：桶排序
/**
 * 初始化一个数组，然后放入数字，=0不输出，=1输出
 */
//system("pause")可以暂停程序

int a[100],n;//
void maopao(int a[]){
	int b[100]=a;

}

int main() {
	scanf("%d",&n);//输入一个数
	for(i=1;i<=n;i++){
		scanf("%d",&a[i]);
	}

	return 0;
}
