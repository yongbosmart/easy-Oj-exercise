#include<stdio.h>
#include <queue>
#include<stack>
#include <iostream>
using namespace std;

void card_game(){
	int l,m,n;
	scanf("%d",&l);
	queue<int> a;
	queue<int> b;
	for(int i=0;i<l;i++){
		scanf("%d",&m);
		a.push(m);
	}
	scanf("%d",&n);
	for(int i=0;i<l;i++){
		scanf("%d",&n);
		a.push(n);
	}
}


int main() {
//	cout << "!!!queue_stack_ex1!!!" << endl; // prints !!!queue_stack_ex1!!!

	return 0;
}
